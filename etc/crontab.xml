<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="mailcampaigns">
        <job name="mailcampaigns_historical_sync_cron" instance="MailCampaigns\Magento2Connector\Cron\HistoricalSyncCron" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="mailcampaigns_post_cron" instance="MailCampaigns\Magento2Connector\Cron\PostCron" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="mailcampaigns_status_cron" instance="MailCampaigns\Magento2Connector\Cron\StatusCron" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="mailcampaigns_quote_cron" instance="MailCampaigns\Magento2Connector\Cron\QuoteCron" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="mailcampaigns_order_cron" instance="MailCampaigns\Magento2Connector\Cron\OrderCron" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="mailcampaigns_product_cron" instance="MailCampaigns\Magento2Connector\Cron\ProductCron" method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>
